{
	"name": "uwebsocket-plus",
	"version": "0.0.3",
	"description": "A powerful, plugin-based WebSocket library built on top of Elysia with TypeScript support. UWebSocket+ provides advanced room management, real-time messaging, and extensible plugin architecture.",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -p tsconfig.build.json --watch",
		"dev": "bun run examples/chat-app/server.ts",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "bun run build"
	},
	"keywords": [
		"websocket",
		"elysia",
		"bun",
		"typescript",
		"realtime",
		"chat",
		"rooms",
		"plugin",
		"socket"
	],
	"author": {
		"name": "clqu",
		"email": "mail@clqu.dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/clqu/uwebsocket-plus.git"
	},
	"homepage": "https://github.com/clqu/uwebsocket-plus",
	"bugs": {
		"url": "https://github.com/clqu/uwebsocket-plus/issues"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/bun": "latest",
		"@types/uuid": "^10.0.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"elysia": "^1.3.8",
		"uuid": "^11.1.0",
		"uwebsocket-plus": "^0.0.3"
	},
	"engines": {
		"node": ">=18",
		"bun": ">=1.0.0"
	}
}